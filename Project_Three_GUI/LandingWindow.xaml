<Window x:Class="Project_Three_GUI.LandingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Three_GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Center" Height="23" Margin="0,-30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" x:Name="Username"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Center" Width="75"/>
        <TextBox HorizontalAlignment="Center" Height="23" Margin="0,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" x:Name="Password"/>
    </Grid>
</Window>
