<Application x:Class="Project_Three_GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Project_Three_GUI"
             StartupUri="LandingWindow.xaml">
    <Application.Resources>
        <Style x:Key="ButtonColor">
            <Setter Property="Control.Background" Value="#FFFFFFFF"/>
        </Style>
        <Style x:Key="WindowPrimaryBackground">
            <Setter Property="Control.Background" Value="Maroon"/>
        </Style>
        <Style x:Key="WindowHeightSize">
            <Setter Property="Window.Height" Value="450"/>
        </Style>
    </Application.Resources>
</Application>
